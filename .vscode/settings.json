{
    "code-runner.executorMap": {
        "cpp": "cd $dir && clang++ $fileName -std=gnu++20 -Wall -Wextra -O2 -I $workspaceRoot/ac-library -I /opt/homebrew/Cellar/boost/1.87.0/include/ -I $workspaceRoot/include/ && ./a.out < input.txt && rm ./a.out",
        "python": "cd $dir && python3 -u $fileName < input.txt"
    },
    "files.associations": {
        "*.cpp": "cpp",
        "*.tcc": "cpp",
        "iostream": "cpp",
        "iterator": "cpp",
        "random": "cpp",
        "vector": "cpp",
        "all": "cpp",
        "ostream": "cpp",
        "tuple": "cpp",
        "string": "cpp",
        "deque": "cpp",
        "iomanip": "cpp",
        "chrono": "cpp",
        "bitset": "cpp",
        "unordered_set": "cpp",
        "set": "cpp"
    }
}